{
  "name": "@dean0x/dot",
  "version": "0.1.0",
  "description": "dot.ai - AI-powered code generation from .ai specification files",
  "main": "dist/index.js",
  "bin": {
    "dot": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepare": "npm run build",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "ai",
    "code-generation",
    "claude",
    "automation",
    "cli"
  ],
  "author": "dean0x <https://github.com/dean0x>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dean0x/dot.ai.git"
  },
  "bugs": {
    "url": "https://github.com/dean0x/dot.ai/issues"
  },
  "homepage": "https://github.com/dean0x/dot.ai#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "diff": "^5.2.0",
    "ora": "^5.4.1",
    "p-limit": "^7.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/diff": "^5.0.9",
    "@types/node": "^20.11.0",
    "@types/ora": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
